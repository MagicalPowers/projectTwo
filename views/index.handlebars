
<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h2>This is our Home Page!</h2>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="usercreate">Create User</label>
            <input type="text" id="usercreate" class="form-control" aria-describedby="usercreate" placeholder="Create User">
          </div>
          <button id="submituser" class="btn btn-primary float-right">Submit</button>
        </form>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="username">Username</label>
            <input type="text" id="username" class="form-control" aria-describedby="username" placeholder="Username">
          </div>
          <div class="form-group"></div>
            <label for="post-text">Post Name</label>
            <input type="text" id="post-text" class="form-control" aria-describedby="post-text" placeholder="Enter Your Headline">
          </div>
          <div class="form-group">
            <label for="post-description">Post Body</label>
            <textarea class="form-control" id="post-description" aria-describedby="post-description" placeholder="Enter Your Personal"></textarea>
          </div>
          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>
        <h3 class="clearfix">Examples:</h3>
        <ul id="post-list" class="list-group">
          {{#each Post}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="example/{{this.UserId}}">{{this.name}}</a>
            <h5>{{this.title}}</h5>
            <p>{{this.body}}</p>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>
